@article{chenDiscoveringSpatiotemporalImpacts2019,
  title = {Discovering the Spatio-Temporal Impacts of Built Environment on Metro Ridership Using Smart Card Data},
  author = {Chen, Enhui and Ye, Zhirui and Wang, Chao and Zhang, Wenbo},
  date = {2019-12-01},
  journaltitle = {Cities},
  shortjournal = {Cities},
  volume = {95},
