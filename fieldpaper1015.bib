  keywords = {Human dynamics,Human mobility,Origin–destination matrices,Random walks},
  file = {C\:\\Users\\qs26\\Zotero\\storage\\B9V3GYE9\\Barbosa et al. - 2018 - Human mobility Models and applications.pdf;C\:\\Users\\qs26\\Zotero\\storage\\UFLUDGJM\\S037015731830022X.html;C\:\\Users\\qs26\\Zotero\\storage\\Y6FP6FAL\\S037015731830022X.html}
}

@book{ben-akivaDiscreteChoiceAnalysis1985,
