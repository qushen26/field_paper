@article{perumalContextualDensityUS2017,
  title = {Contextual {{Density}} and {{US Automotive CO2 Emissions}} across the {{Rural}}–{{Urban Continuum}}},
  author = {Perumal, Andrew and Timmons, David},
  date = {2017-11-01},
  journaltitle = {International Regional Science Review},
