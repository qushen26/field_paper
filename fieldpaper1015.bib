@article{lenormandSystematicComparisonTrip2016,
  title = {Systematic Comparison of Trip Distribution Laws and Models},
  author = {Lenormand, Maxime and Bassolas, Aleix and Ramasco, José J.},
  date = {2016-02-01},
  journaltitle = {Journal of Transport Geography},
  shortjournal = {Journal of Transport Geography},
  volume = {51},
