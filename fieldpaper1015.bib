@article{proffittAccessibilityPlanningAmerican2019,
  ids = {proffittAccessibilityPlanningAmerican2019a},
  title = {Accessibility Planning in {{American}} Metropolitan Areas: Are We There Yet?},
  shorttitle = {Accessibility Planning in {{American}} Metropolitan Areas},
  author = {Proffitt, David G and Bartholomew, Keith and Ewing, Reid and Miller, Harvey J},
