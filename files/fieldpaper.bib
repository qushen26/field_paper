	year = {2011},
	keywords = {Neighborhood Characteristics, Population, Regional Economic Activity: Growth, Development, and Changes, Size and Spatial Distributions of Regional Economic Activity, Urban, Rural, and Regional Economics: Regional Migration, Regional Labor Markets},
