  author = {Cerin, Ester and Conway, Terry L. and Adams, Marc A. and Barnett, Anthony and Cain, Kelli L. and Owen, Neville and Christiansen, Lars B. and van Dyck, Delfien and Mitáš, Josef and Sarmiento, Olga L. and Davey, Rachel C. and Reis, Rodrigo and Salvo, Deborah and Schofield, Grant and Sallis, James F.},
  options = {useprefix=true},
  date = {2018-08-01},
  journaltitle = {Social Science \& Medicine},
