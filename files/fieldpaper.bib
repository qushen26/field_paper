@article{gordonGasolineConsumptionCities1989,
	title = {Gasoline {Consumption} {And} {Cities}: {A} {Reply}},
	volume = {55},
	copyright = {Copyright American Planning Association Summer 1989},
	issn = {01944363},
	shorttitle = {Gasoline {Consumption} {And} {Cities}},
	url = {http://search.proquest.com/docview/229599978/abstract/FBB96AB65F044006PQ/1},
	abstract = {In an article entitled "Gasoline Consumption and Cities," Peter W. G. Newman and Jeffrey R. Kenworthy (NK) compared gasoline consumption per capita across cities and countries. They found that the average consumption in US cities is about twice as high as in Australian cities and in Toronto; 4 times higher than in European cities; and 10 times higher than in 3 "westernized" Asian cities. NK suggested that large variances of gas consumption within 10 large US cities exist because of land use patterns and the transportation system. They established a policy objective of minimizing gasoline consumption and recommended a twin strategy of "reurbanization" and investment in urban rail transit. An analysis of the wisdom of fuel use minimization, the validity of global comparisons, the impact of urban structure on travel, the potential for rail transit and modal choice, and the land impact of transit development suggest that NK's analysis is faulty and that their policy and planning prescriptions are inappropriate and unfeasible.},
