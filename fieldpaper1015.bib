@article{schmidheinyEquivalenceLocationChoice2011,
  title = {On the Equivalence of Location Choice Models: Conditional Logit, Nested Logit and {{Poisson}}},
  shorttitle = {On the Equivalence of Location Choice Models},
  author = {Schmidheiny, Kurt and Brülhart, Marius},
  date = {2011-03-01},
  journaltitle = {Journal of Urban Economics},
