	author = {Xu, Pengpeng and Huang, Helai and Dong, Ni},
	month = feb,
	year = {2018},
	keywords = {Modifiable areal unit problem, Review, Macro level crash, Prediction model, Traffic safety, Traffic safety analysis zones},
	pages = {73--82},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\qs26\\Zotero\\storage\\FDKKJBF3\\Xu et al. - 2018 - The modifiable areal unit problem in traffic safet.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\qs26\\Zotero\\storage\\FYRZ8V66\\S2095756415306322.html:text/html},
}

