  keywords = {Macro level crash,MAUP,Modifiable areal unit problem,Prediction model,Traffic safety,Traffic safety analysis zones},
  file = {C\:\\Users\\qs26\\Zotero\\storage\\FDKKJBF3\\Xu et al. - 2018 - The modifiable areal unit problem in traffic safet.pdf;C\:\\Users\\qs26\\Zotero\\storage\\FYRZ8V66\\S2095756415306322.html}
}

@article{yangLimitsPredictabilityCommuting2014,
