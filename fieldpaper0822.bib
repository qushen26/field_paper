	file = {SAGE PDF Full Text:C\:\\Users\\qs26\\Zotero\\storage\\WUEEHGXY\\Zhou and Kockelman - 2008 - Self-Selection in Home Choice Use of Treatment Ef.pdf:application/pdf},
}

@article{tianTrafficGeneratedMixedUse2015,
