@article{semenescu30YearsSoft2020,
  title = {30 {{Years}} of Soft Interventions to Reduce Car Use – {{A}} Systematic Review and Meta-Analysis},
  author = {Semenescu, Alin and Gavreliuc, Alin and Sârbescu, Paul},
  date = {2020-08-01},
  journaltitle = {Transportation Research Part D: Transport and Environment},
  shortjournal = {Transportation Research Part D: Transport and Environment},
  volume = {85},
  pages = {102397},
