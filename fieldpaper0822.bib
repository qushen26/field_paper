	month = jul,
	year = {2012},
	note = {Num Pages: e40393
Place: San Francisco, United States
Publisher: Public Library of Science
Section: Research Article},
	keywords = {Mexico, Urban geography, Brazil, Colombia, Computational linguistics, Homicide, Probability distribution, Statistical distributions},
	pages = {e40393},
	file = {Full Text PDF:C\:\\Users\\qs26\\Zotero\\storage\\E873HQB7\\Gomez-Lievano et al. - 2012 - The Statistics of Urban Scaling and Their Connecti.pdf:application/pdf},
