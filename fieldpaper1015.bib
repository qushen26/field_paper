  shorttitle = {International Comparisons of the Associations between Objective Measures of the Built Environment and Transport-Related Walking and Cycling},
  author = {Christiansen, Lars B. and Cerin, Ester and Badland, Hannah and Kerr, Jacqueline and Davey, Rachel and Troelsen, Jens and van Dyck, Delfien and Mitáš, Josef and Schofield, Grant and Sugiyama, Takemi and Salvo, Deborah and Sarmiento, Olga L. and Reis, Rodrigo and Adams, Marc and Frank, Larry and Sallis, James F.},
