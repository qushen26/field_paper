@article{jiangZipfLawAll2015,
	title = {Zipf{\textquoteright}s law for all the natural cities around the world},
	volume = {29},
	issn = {13658816},
	url = {http://stats.lib.pdx.edu/proxy.php?url=http://search.ebscohost.com/login.aspx?direct=true&db=aph&AN=102777792&site=ehost-live},
	doi = {10.1080/13658816.2014.988715},
	abstract = {Two fundamental issues surrounding research on Zipf{\textquoteright}s law regarding city sizes are whether and why this law holds. This paper does not deal with the latter issue with respect to why, and instead investigates whether Zipf{\textquoteright}s law holds in a global setting, thus involving all cities around the world. Unlike previous studies, which have mainly relied on conventional census data such as populations and census-bureau-imposed definitions of cities, we adopt naturally (in terms of data speak for itself) delineated cities, or natural cities, to be more precise, in order to examine Zipf{\textquoteright}s law. We find that Zipf{\textquoteright}s law holds remarkably well for all natural cities at the global level, and it remains almost valid at the continental level except for Africa at certain time instants. We further examine the law at the country level, and note that Zipf{\textquoteright}s law is violated from country to country or from time to time. This violation is mainly due to our limitations; we are limited to individual countries, or to a static view on city-size distributions. The central argument of this paper is that Zipf{\textquoteright}s law is universal, and we therefore must use the correct scope in order to observe it. We further find Zipf{\textquoteright}s law applied to city numbers; the number of cities in the largest country is twice as many as that in the second largest country, three times as many as that in the third largest country, and so on. These findings have profound implications for big data and the science of cities.},
	number = {3},
	urldate = {2021-04-23},
	journal = {International Journal of Geographical Information Science},
	author = {Jiang, Bin and Yin, Junjun and Liu, Qingling},
	month = mar,
	year = {2015},
	note = {Publisher: Taylor \& Francis Ltd},
	keywords = {big data, BIG data, CITIES \& towns, city-size distributions, head/tail breaks, head/tail division rule, IMAGE processing, nighttime imagery, REMOTE-sensing images, ZIPF'S law},
	pages = {498--522},
	file = {EBSCO Full Text:C\:\\Users\\qs26\\Zotero\\storage\\9HJFGXZB\\Jiang et al. - 2015 - Zipf{\textquoteright}s law for all the natural cities around the w.pdf:application/pdf},
}

